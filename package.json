    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.4.2",
    "storybook": "^8.6.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.5",
