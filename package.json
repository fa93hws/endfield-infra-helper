{
  "name": "endfield-industry-helper",
  "version": "1.0.0",
  "description": "Responsive React application with TypeScript and Material-UI",
  "type": "module",
  "scripts": {
    "page:dev": "vike dev",
    "page:build": "tsc && vike build",
    "lint:ts:type": "tsc --noEmit",
    "lint:ts": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:ts:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage",
    "storybook:dev": "storybook dev -p 6006 -c config/storybook",
    "storybook:build": "storybook build -c config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vike": "^0.4.208",
    "vike-react": "^0.5.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@testing-library/jest-dom": "^6.9.1",
